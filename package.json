{
  "name": "react-upload-progress",
  "version": "0.0.2",
  "description": "React upload progress observer based on render props",
  "main": "dist/index.js",
  "scripts": {
    "format": "prettier --single-quote --trailing-comma es5 --write src/*.js",
    "build:prod": "NODE_ENV=production babel src --out-dir dist --ignore index.test.js",
    "build": "NODE_ENV=development babel src --out-dir dist --ignore index.test.js --source-maps",
    "test": "jest",
    "setup": "npm install && cd examples && npm install && cd ..",
    "start": "cd examples && npm start"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/whitedogg13/react-upload-progress"
  },
  "keywords": [
    "react",
    "file-upload",
    "upload"
  ],
  "author": "Richie Hsieh",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/whitedogg13/react-upload-progress/issues"
  },
  "homepage": "https://github.com/whitedogg13/react-upload-progress",
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-jest": "^19.0.0",
    "babel-plugin-async-to-promises": "^1.0.5",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "jest": "^22.4.3",
    "prettier": "^0.22.0",
    "react": "^15.4.2",
    "react-addons-test-utils": "^15.4.2",
    "react-dom": "^15.4.2",
    "prop-types": "^15.4.2"
  }
}
